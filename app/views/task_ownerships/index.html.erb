<h1>Listing task_ownerships</h1>

<table>
  <tr>
    <th>User</th>
    <th>Task</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @task_ownerships.each do |task_ownership| %>
  <tr>
    <td><%= task_ownership.user_id %></td>
    <td><%= task_ownership.task_id %></td>
    <td><%= link_to 'Show', task_ownership %></td>
    <td><%= link_to 'Edit', edit_task_ownership_path(task_ownership) %></td>
    <td><%= link_to 'Destroy', task_ownership, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

